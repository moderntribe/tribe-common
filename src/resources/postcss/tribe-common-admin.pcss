/* -- Freemius tweaks -- */
body.tribe-welcome #fs_connect {
	border: 1px solid #e1e1e4;
	box-sizing: border-box;
	border-radius: 16px;
	margin-left: 22px;
	box-shadow: none;

	.fs-actions {
		background-color: transparent;
	}

	.fs-permissions {
		border-top: 1px solid #e1e1e4;
		margin: 0 16px;
	}

	button {
		background-color: #3d54ff;
		border-color: #3d54ff;
	}

	.button-secondary {
		border-color: #3d54ff;
		color: #3d54ff;
		background: white;
	}

	a {
		color: #3d54ff;

		&:focus {
			box-shadow: none;
			outline: none;
		}

		&:hover {
			color: #161b7d;
		}
	}
}

/* -- CSS Switch based off https://codepen.io/mallendeo/pen/eLIiG  -- */
input[type="checkbox"].tribe-common-switch__input {
	display: none;

	+ .tribe-common-switch__label {
		background: #fff;
		border: 1px solid #aaa;
		border-radius: 4px;
		box-sizing: border-box;
		cursor: pointer;
		display: block;
		height: 18px;
		outline: 0;
		padding: 3px;
		position: relative;
		transition: all 0.2s ease;
		user-select: none;
		width: 27px;

		&:after,
		&:before {
			content: "";
			display: block;
			height: 10px;
			position: relative;
			width: 10px;
		}

		&:after {
			background: #878787;
			border-radius: 2px;
			content: "";
			left: 0;
			transition: all 0.2s ease;
		}

		&:before {
			display: none;
		}

		&::selection {
			background: none;
		}
	}

	&:checked + .tribe-common-switch__label {

		&:after {
			background: #2e709d;
			left: 50%;
		}
	}
}
