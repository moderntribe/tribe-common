	right: 22px;
	top: 20px;
	position: absolute;
	pointer-events: none;
	width: 14px;
	height: 13px;

	@media only screen and (max-width: 480px) {
		right: 25px;
	}
}

.tribe-events-admin__recent-log-filters-select-wrapper select {
	font-size: 14px;
	line-height: 1.14;
	color: #0f1031;
	padding: 18px 22px 18px 23px;
	border: 1px solid #e1e1e4;
	border-radius: 16px;
	width: 300px;
	-webkit-appearance: none;
	background: #fff;

	@media only screen and (max-width: 768px) {
		width: 500px;
	}

	@media only screen and (max-width: 480px) {
		width: 300px;
	}
}

.tribe-events-admin__recent-log-filters-select-wrapper select:focus-visivble {
	outline: none;
}

.tribe-events-admin__recent-log-filters-select-wrapper select option {
	font-size: 14px;
	line-height: 1.14;
	color: #0f1031;
}

.tribe-events-admin__ea-status {
	margin-top: 100px;
}

/* issues found section */
.tribe-events-admin__issues-found-card {
	background-color: #f3eee8;
  	border-radius: 8px;
	margin-bottom: 20px;
}

.tribe-events-admin__issues-found-card:last-of-type {
	margin-bottom: 100px;
}

.tribe-events-admin__issues-found-card-title {
	position: relative;
	display: flex;
	align-items: center;
	padding: 10px 20px 10px 17px;
	cursor: pointer;
}

.tribe-events-admin__issues-found-card-title img {
	width: 21px;
	height: 21px;
	object-fit: contain;
	margin-right: 14px;
}

.tribe-events-admin__issues-found-card-title h3 {
	margin: 0;

	@media only screen and (max-width: 768px)  {
		max-width: 90%;
	}

	@media only screen and (max-width: 480px)  {
		max-width: 80%;
	}
}

.tribe-events-admin__issues-found-card-title span {
	color: #0f1031;
	display: block;
}

.tribe-events-admin__issues-found-card-title i {
	position: absolute;
	background-image: url("../images/help/arrow-down.svg");
	background-repeat: no-repeat;
	background-position: center;
	background-size: contain;
	width: 15px;
	height: 15px;
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
	-o-transition: all 0.3s ease;
	-ms-transition: all 0.3s ease;
	transition: all 0.3s ease;
	margin: 12px 20px;
	right: 0;
	top: 0;
}

.tribe-events-admin__issues-found-card-title.active i {
	background-image: url("../images/help/arrow-up.svg");
	background-repeat: no-repeat;
	top: 5px;
}

.tribe-events-admin__issues-found-card-description {
	padding: 0 20px 20px 55px;
	display: none;
}

.tribe-events-admin__issues-found-card-description p {
	font-size: 16px;
	color: #0f1031;
	margin: 0;
}

.tribe-events-admin__issues-found-card-description-actions {
	display: flex;
	padding: 20px 0 10px;
}

.tribe-events-admin__issues-found-card-description-actions a {
	font-size: 16px;
	text-decoration: none;
	color: #334aff;
	margin-right: 20px;
	padding-bottom: 5px;
    border-bottom: 2px solid #334aff;

	&:focus {
		box-shadow: none;
		outline: none;
	}

	&:hover {
		color: #1c39bb;
		border-bottom: 2px solid #1c39bb;
	}
}

.tribe-events-admin__ea-status-table {
	border-radius: 16px;
	border: 1px solid #e1e1e4;
	margin: 30px 0 40px;
	width: 100%;
	padding: 20px 0;
}

.tribe-events-admin__ea-status-table a {
	text-decoration: none;
	color: #334aff;
	padding-bottom: 2px;
    border-bottom: 2px solid #334aff;

	&:focus {
		box-shadow: none;
		outline: none;
	}

	&:hover {
		color: #1c39bb;
		border-bottom: 2px solid #1c39bb;
	}
}

.tribe-events-admin__ea-status-table tr {
	display: flex;
	align-items: center;
	margin: 12px 0;
	padding: 5px 25px;

	@media only screen and (max-width: 480px)  {
		flex-direction: column;
		align-items: flex-start;
	}
}

.tribe-events-admin__ea-status-table th {
	font-weight: bold;
	line-height: 1.17;
	color: #0f1031;
}

.tribe-events-admin__ea-status-table td {
	font-size: 16px;
	line-height: 1.63;
	color: #0f1031;
	display: flex;
	align-items: center;
	width: 25%;

	@media only screen and (max-width: 480px)  {
		width: 100%;
		flex-direction: column;
		align-items: flex-start;
	}
}

.tribe-events-admin__ea-status-table td:nth-child(2){
	width: 50%;

	@media only screen and (max-width: 480px)  {
		width: 100%;
	}
}

.tribe-events-admin__ea-status-table-dark{
	background-color: #f9f7f4;
}

.tribe-events-admin__ea-status-table td img {
	width: 21px;
	height: 21px;
	object-fit: contain;
	margin-right: 14px;

	@media only screen and (max-width: 480px) {
		margin: 10px 0;
	}
}

.tribe_events_page_tribe-troubleshooting {
	background-color: #FFF;
}

#tribe-ticketing,
#tribe-community {
	display: none;
}
